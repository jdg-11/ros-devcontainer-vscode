{
	"name": "ROS devcontainer for vscode",
	"dockerComposeFile": [
		"../docker-compose.yml"
	],
	"service": "workspace",
	"workspaceFolder": "/workspace",
	"shutdownAction": "stopCompose",
	"extensions": [
		"ms-vscode.cpptools",
		"ms-python.python",
		"ajshort.msg",
		"twxs.cmake",
		"redhat.vscode-xml",
		"actboy168.tasks",
		"brpaz.file-templates",
		"ms-iot.vscode-ros",
		"yzhang.markdown-all-in-one",
		"bierner.markdown-mermaid"
    ],
    //"appPort": [80],
}
